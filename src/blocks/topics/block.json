{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"textdomain": "unitone-bbpress-integrator",
	"name": "unitone-bbpress-integrator/topics",
	"title": "(bbPress) Recent Topics",
	"description": "A list of recent topics, sorted by: newness, popularity, or recent replies.",
	"attributes": {
		"title": {
			"type": "string"
		},
		"orderBy": {
			"type": "string"
		},
		"parentForum": {
			"type": "string",
			"default": "any"
		},
		"maxShown": {
			"type": "number",
			"default": 5
		},
		"showDate": {
			"type": "boolean",
			"default": false
		},
		"showUser": {
			"type": "boolean",
			"default": false
		}
	},
	"supports": {
		"className": false,
		"anchor": true
	},
	"category": "unitone/widget",
	"editorScript": "file:./index.js",
	"render": "file:./render.php"
}
